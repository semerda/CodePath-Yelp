# Uncomment this line to define a global platform for your project
# platform :ios, '9.0'

target 'Yelp' do

    # Comment this line if you're not using Swift and don't want to use dynamic frameworks
    use_frameworks!

    # https://github.com/AFNetworking/AFNetworking
    # This pulls in the latest version of AFNetworking >= 2.6 but less than 3.0.
    pod 'AFNetworking', '~> 2.6'

    # https://github.com/bdbergeron/BDBOAuth1Manager
    # OAuth 1.0a library for AFNetworking 2.x    
    pod 'BDBOAuth1Manager'
  
    # https://github.com/jdg/MBProgressHUD
    pod 'MBProgressHUD'

    # https://github.com/SwiftyJSON/SwiftyJSON
    #pod 'SwiftyJSON'

    # https://github.com/dzenbot/DZNEmptyDataSet
    #pod 'DZNEmptyDataSet'

    # https://github.com/intuit/LocationManager
    pod 'INTULocationManager'

    # https://github.com/bvogelzang/SevenSwitch
    #pod 'SevenSwitch'

    target 'YelpTests' do
        inherit! :search_paths
        # Pods for testing
    end

end
